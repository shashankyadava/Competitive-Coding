int main(){
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    ll n,sum; cin>>n>>sum;
    vector<ll>v(n);
    for(int i = 0; i<n; i++){
        cin>>v[i];
    }
    dp[0] = 1;
    for(ll weight = 0; weight<=sum; weight++){
        for(ll i = 0; i<n; i++){
            if(weight-v[i]>=0){
                dp[weight] = dp[weight]+dp[weight-v[i]];
                dp[weight]%=mod;
            }
        }
    }
    cout<<dp[sum]<<endl;
    return 0;
}
